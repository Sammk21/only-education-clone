    "rankComparison": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.rank-comparison",
      "required": true
    },
    "eligibilityCriteria": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.eligibility-card",
      "required": true
    },
    "documentRequired": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.popular-courses",
      "required": true
    },
