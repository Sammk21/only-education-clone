{
  "kind": "collectionType",
  "collectionName": "universities",
  "info": {
    "singularName": "university",
    "pluralName": "universities",
    "displayName": "University",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "universityProfile": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.profiles",
      "required": true
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.cta",
      "required": false
    },
    "overview": {
      "type": "component",
      "repeatable": false,
      "component": "shared.header",
      "required": true
    },
    "whythisUniversity": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.why-us",
      "required": true
    },
    "rankComparison": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.rank-comparison",
      "required": true
    },
    "eligibilityCriteria": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.eligibility-card",
      "required": true
    },
    "documentRequired": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.popular-courses",
      "required": true
    },
    "feesStructure": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.eligibility-table",
      "required": true
    },
    "faq": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.faq",
      "required": true
    },
    "countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country",
      "mappedBy": "university"
    },
    "noOfStudentsStudying": {
      "type": "string",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": false
    },
    "authorName": {
      "type": "enumeration",
      "enum": [
        "Omkar",
        "Sam",
        "Sarfraj",
        "Adesh",
        "Dhanashree",
        "Afreen",
        "Ketkki",
        "Premita",
        "Yogita",
        "Zhankana",
        "Parveen"
      ],
      "required": true
    },
    "universityInfo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.list"
    },
    "placement": {
      "type": "component",
      "repeatable": false,
      "component": "shared.placement"
    },
    "streams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stream.stream",
      "mappedBy": "university"
    },
    "ownership": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ownership.ownership"
    },
    "exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam.exam",
      "mappedBy": "university"
    },
    "searchableImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "indian_state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::indian-state.indian-state",
      "inversedBy": "universities"
    },
    "highlights": {
      "type": "component",
      "repeatable": false,
      "component": "shared.highlights"
    },
    "affiliated": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::university.university"
    },
    "overviewTabs": {
      "displayName": "Overview Tabs",
      "type": "component",
      "repeatable": false,
      "component": "university.overview-tabs"
    },
    "coursesFees": {
      "displayName": "coursesFees",
      "type": "component",
      "repeatable": false,
      "component": "university.courses-fees"
    },
    "admission": {
      "displayName": "admission",
      "type": "component",
      "repeatable": false,
      "component": "university.admission"
    },
    "placements": {
      "displayName": "placements",
      "type": "component",
      "repeatable": false,
      "component": "university.placements"
    },
    "gallery": {
      "displayName": "gallery",
      "type": "component",
      "repeatable": false,
      "component": "university.gallery"
    },
    "faculty": {
      "displayName": "faculty",
      "type": "component",
      "repeatable": false,
      "component": "university.faculty"
    },
    "hostel": {
      "displayName": "hostel",
      "type": "component",
      "repeatable": false,
      "component": "university.hostel"
    },
    "scholarships": {
      "displayName": "scholarships",
      "type": "component",
      "repeatable": false,
      "component": "university.scholarships"
    },
    "notification": {
      "displayName": "notification",
      "type": "component",
      "repeatable": false,
      "component": "university.notification"
    },
    "ranking": {
      "type": "component",
      "repeatable": true,
      "component": "university.ranking"
    }
  }
}
