{
  "kind": "collectionType",
  "collectionName": "universities",
  "info": {
    "singularName": "university",
    "pluralName": "universities",
    "displayName": "University",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "universityProfile": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.profiles",
      "required": true
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.cta",
      "required": false
    },
    "faq": {
      "type": "component",
      "repeatable": false,
      "component": "blocks.faq",
      "required": true
    },
    "countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country",
      "mappedBy": "university"
    },
    "noOfStudentsStudying": {
      "type": "string",
      "required": false
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": false
    },
    "authorName": {
      "type": "enumeration",
      "enum": [
        "Omkar",
        "Sam",
        "Sarfraj",
        "Adesh",
        "Dhanashree",
        "Afreen",
        "Ketkki",
        "Premita",
        "Yogita",
        "Zhankana",
        "Parveen",
        "Dweep",
        "Raviraj"
      ],
      "required": true
    },
    "streams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stream.stream",
      "mappedBy": "university"
    },
    "ownership": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ownership.ownership"
    },
    "exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam.exam",
      "mappedBy": "university"
    },
    "searchableImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "indian_state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::indian-state.indian-state",
      "inversedBy": "universities"
    },
    "affiliated": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::university.university"
    },
    "overviewTabs": {
      "displayName": "Overview Tabs",
      "type": "component",
      "repeatable": false,
      "component": "university.overview-tabs",
      "required": true
    },
    "coursesFees": {
      "displayName": "coursesFees",
      "type": "component",
      "repeatable": false,
      "component": "university.courses-fees",
      "required": true
    },
    "admission": {
      "displayName": "admission",
      "type": "component",
      "repeatable": false,
      "component": "university.admission",
      "required": true
    },
    "placements": {
      "displayName": "placements",
      "type": "component",
      "repeatable": false,
      "component": "university.placements",
      "required": true
    },
    "gallery": {
      "displayName": "gallery",
      "type": "component",
      "repeatable": false,
      "component": "university.gallery",
      "required": true
    },
    "faculty": {
      "displayName": "faculty",
      "type": "component",
      "repeatable": false,
      "component": "university.faculty",
      "required": true
    },
    "hostel": {
      "displayName": "hostel",
      "type": "component",
      "repeatable": false,
      "component": "university.hostel",
      "required": true
    },
    "scholarships": {
      "displayName": "scholarships",
      "type": "component",
      "repeatable": false,
      "component": "university.scholarships",
      "required": true
    },
    "ranking": {
      "type": "component",
      "repeatable": true,
      "component": "university.ranking",
      "required": true
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "university"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "university"
    }
  }
}
