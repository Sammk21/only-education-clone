{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "unique": false,
      "configurable": false,
      "required": false
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "author"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "author"
    },
    "picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "job": {
      "type": "string",
      "required": false
    },
    "firstName": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "lastName": {
      "type": "string",
      "required": false
    },
    "enquiries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enquiry.enquiry",
      "mappedBy": "users_permissions_user"
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "verified": {
      "type": "boolean",
      "default": false
    },
    "last_otp_request": {
      "type": "datetime"
    },
    "resend_attempts": {
      "type": "integer"
    },
    "otp_session": {
      "type": "string"
    }
  }
}
